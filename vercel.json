{
  "buildCommand": "cp -r app api/ && cp -r templates api/ && cp -r static api/",
  "devCommand": "uvicorn app.main:app --reload --host 0.0.0.0 --port 3000",
  "installCommand": "pip install -r requirements.txt",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ]
}
